{
    "name": "jupiter-admin-dashboard",
    "version": "1.3.1",
    "main": "main.js",
    "scripts": {
        "postinstall": "electron-builder install-app-deps",
        "ng": "ng",
        "start": "npm-run-all -p electron:serve ng:serve",
        "build": "npm run electron:serve-tsc && ng build --base-href ./",
        "build:dev": "npm run build -- -c dev",
        "build:prod": "npm run build -- -c production",
        "ng:serve": "ng serve",
        "ng:serve:web": "ng serve -c web",
        "electron:serve-tsc": "tsc -p tsconfig-serve.json",
        "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && npx electron . --serve",
        "electron:local": "npm run build:prod && npx electron .",
        "electron:build": "npm run build:prod && electron-builder build",
        "test": "ng test --watch=false",
        "test:watch": "ng test",
        "e2e": "npm run build:prod && cross-env TS_NODE_PROJECT='e2e/tsconfig.e2e.json' mocha --timeout 300000 --require ts-node/register e2e/**/*.e2e.ts",
        "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
        "lint": "ng lint",
        "release": "electron:build"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~9.1.12",
        "@angular/cdk": "^9.0.0",
        "@angular/common": "~9.1.12",
        "@angular/compiler": "~9.1.12",
        "@angular/core": "~9.1.12",
        "@angular/forms": "~9.1.12",
        "@angular/localize": "^9.1.12",
        "@angular/platform-browser": "~9.1.12",
        "@angular/platform-browser-dynamic": "~9.1.12",
        "@angular/router": "~9.1.12",
        "@auth0/angular-jwt": "^4.2.0",
        "@fullcalendar/angular": "^5.1.0",
        "@fullcalendar/core": "^5.1.0",
        "@fullcalendar/daygrid": "^5.1.0",
        "@nebular/bootstrap": "5.1.0",
        "@nebular/eva-icons": "5.1.0",
        "@nebular/moment": "^5.1.0",
        "@nebular/theme": "^5.1.0",
        "@ng-bootstrap/ng-bootstrap": "^6.2.0",
        "@swimlane/ngx-datatable": "^17.1.0",
        "angular2-virtual-scroll": "git+https://github.com/AndreaScotti/angular2-virtual-scroll.git",
        "bootstrap": "^4.5.0",
        "classlist.js": "^1.1.20150312",
        "core-js": "^2.6.11",
        "electron-store": "^6.0.0",
        "eva-icons": "^1.1.3",
        "ionicons": "2.0.1",
        "jquery": "^1.12.4",
        "lodash": "^4.17.19",
        "moment": "^2.27.0",
        "nebular-icons": "^1.1.0",
        "ngx-ace-wrapper": "^8.0.0",
        "ngx-chips": "^2.1.0",
        "ngx-moment": "^3.5.0",
        "npm-run-all": "4.1.5",
        "pace-js": "^1.0.2",
        "popper.js": "^1.16.1",
        "roboto-fontface": "^0.10.0",
        "rxjs": "^6.6.0",
        "spectron": "11.0.0",
        "ts-node": "8.10.2",
        "tslib": "^1.13.0",
        "typeface-exo": "0.0.61",
        "wait-on": "5.0.1",
        "web-animations-js": "^2.3.1",
        "webdriver-manager": "12.1.7",
        "webfontloader": "^1.6.28",
        "zone.js": "~0.10.3"
    },
    "devDependencies": {
        "@angular-builders/custom-webpack": "9.1.0",
        "@angular-devkit/build-angular": "~0.901.12",
        "@angular-eslint/builder": "0.0.1-alpha.32",
        "@angular-eslint/eslint-plugin": "0.0.1-alpha.32",
        "@angular-eslint/eslint-plugin-template": "0.0.1-alpha.32",
