{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "JupiterDashboardAngularApp": {
            "root": "",
            "sourceRoot": "src",
            "projectType": "application",
            "prefix": "jupiter",
            "schematics": {
                "@schematics/angular:component": {
                    "styleext": "scss"
                }
            },
            "architect": {
                "build": {
                    "builder": "@angular-builders/custom-webpack:browser",
                    "options": {
                        "outputPath": "dist",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "tsConfig": "src/tsconfig.app.json",
                        "polyfills": "src/polyfills.ts",
                        "assets": [
                            "src/favicon.ico",
                            "src/favicon.png",
                            "src/assets"
                        ],
                        "styles": [
                            "node_modules/pace-js/templates/pace-theme-flash.tmpl.css",
                            "node_modules/@fortawesome/fontawesome-free/css/all.css",
                            "node_modules/ionicons/scss/ionicons.scss",
                            "node_modules/nebular-icons/scss/nebular-icons.scss",

                            "node_modules/bootstrap/dist/css/bootstrap.css",
                            "node_modules/@nebular/theme/styles/prebuilt/corporate.css",
                            "node_modules/@nebular/bootstrap/styles/prebuilt/corporate.css",
                            "src/styles.scss"
                        ],
                        "scripts": [
                            "node_modules/webfontloader/webfontloader.js",
                            "node_modules/pace-js/pace.min.js"
                        ],
                        "customWebpackConfig": {
                            "path": "./angular.webpack.js"
                        }
                    },
                    "configurations": {
                        "dev": {
                            "optimization": false,
                            "outputHashing": "all",
                            "sourceMap": true,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": false,
                            "fileReplacements": [{
                                "replace": "src/environments/environment.ts",
                                "with": "src/environments/environment.dev.ts"
                            }]
                        },
                        "web": {
                            "optimization": false,
                            "outputHashing": "all",
                            "sourceMap": true,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": false,
                            "fileReplacements": [{
                                "replace": "src/environments/environment.ts",
                                "with": "src/environments/environment.web.ts"
                            }]
                        },
                        "production": {
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "fileReplacements": [{
                                "replace": "src/environments/environment.ts",
                                "with": "src/environments/environment.prod.ts"
                            }]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-builders/custom-webpack:dev-server",
                    "options": {
                        "browserTarget": "JupiterDashboardAngularApp:build"
                    },
                    "configurations": {
                        "dev": {
                            "browserTarget": "JupiterDashboardAngularApp:build:dev"
                        },
                        "web": {
                            "browserTarget": "JupiterDashboardAngularApp:build:web"
                        },
                        "production": {
                            "browserTarget": "JupiterDashboardAngularApp:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "JupiterDashboardAngularApp:build"
                    }
                },
                "test": {
                    "builder": "@angular-builders/custom-webpack:karma",
                    "options": {
                        "main": "src/test.ts",
                        "polyfills": "src/polyfills-test.ts",
                        "tsConfig": "src/tsconfig.spec.json",
                        "karmaConfig": "src/karma.conf.js",
                        "scripts": [
                            "node_modules/webfontloader/webfontloader.js",
                            "node_modules/pace-js/pace.min.js"
                        ],
                        "styles": [
                            "node_modules/pace-js/templates/pace-theme-flash.tmpl.css",
                            "node_modules/@fortawesome/fontawesome-free/css/all.css",
                            "node_modules/ionicons/scss/ionicons.scss",
                            "node_modules/nebular-icons/scss/nebular-icons.scss",
                            "node_modules/bootstrap/dist/css/bootstrap.css",
                            "node_modules/@nebular/theme/styles/prebuilt/corporate.css",
                            "node_modules/@nebular/bootstrap/styles/prebuilt/corporate.css",

                            "src/styles.scss"
                        ],
                        "assets": [
                            "src/favicon.ico",
                            "src/favicon.png",
                            "src/assets"
                        ],
                        "customWebpackConfig": {
                            "path": "./angular.webpack.js",
                            "target": "electron-renderer"
                        }
                    }
                },
                "lint": {
                    "builder": "@angular-eslint/builder:lint",
                    "options": {
                        "eslintConfig": ".eslintrc.json",
                        "tsConfig": [
                            "src/tsconfig.app.json",
                            "src/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        },
        "JupiterDashboardAngularApp-e2e": {
            "root": "e2e",
            "projectType": "application",
            "architect": {
                "lint": {
                    "builder": "@angular-eslint/builder:lint",
                    "options": {
                        "eslintConfig": ".eslintrc.json",
                        "tsConfig": [
                            "e2e/tsconfig.e2e.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        }
    },
    "defaultProject": "JupiterDashboardAngularApp",
    "schematics": {
        "@schematics/angular:component": {
            "prefix": "app",
            "style": "scss"
        },
        "@schematics/angular:directive": {
            "prefix": "app"
        }
    }
}